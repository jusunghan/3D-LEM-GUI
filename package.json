{
  "name": "3dslopeGUI",
  "version": "1.0.0",
  "description": "GUI for 3d slope project",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Slope3D --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Slope3D\"",
    "package-linux": "electron-packager . Slope3D --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Jusung Han",
  "license": "MIT",
  "dependencies": {
    "electron": "^2.0.2"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}
